{
  "name": "ext",
  "displayName": "vscode-diagram",
  "description": "",
  "version": "0.0.1",
  "license": "MIT",
  "publisher": "immament",
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "tsc-compile": "tsc -b tsconfig.build.json",
    "tsc-watch": "tsc -b tsconfig.build.json --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -b tsconfig.test.json",
    "test-watch": "tsc -b tsconfig.test.json --watch ",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src",
    "test": "node ./out/test/vscode/runTest.js",
    "clean": "gts clean",
    "fix": "eslint src --fix"
  },
  "engines": {
    "vscode": "1.54.3"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:diagrams.start"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "diagrams.start",
        "title": "Show diagram",
        "category": "Diagram"
      },
      {
        "command": "diagrams.doRefactor",
        "title": "Refactor",
        "category": "Diagram"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "explorerResourceIsFolder",
          "command": "diagrams.start",
          "group": "diagram"
        }
      ]
    }
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.54.0",
    "chai": "^4.3.4",
    "chalk": "^4.1.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "gts": "^3.1.0",
    "jest": "^26.6.3",
    "mocha": "^8.2.1",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "vscode-test": "^1.5.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "extractor": "^0.0.1",
    "jest-standard-reporter": "^2.0.0"
  }
}
