{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "outDir": "dist",
    "lib": ["es6", "DOM"],
    "sourceMap": true,
    "strict": true /* enable all strict type-checking options */,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["src"],
  "exclude": ["node_modules", ".vscode-test"]
}
